           var hash = {\n              nom: event.target.name.value,\n              prenom: event.target.prenom.value,\n              date: event.target.age.value,\n              age: auj.getFullYear() - nouveau.getFullYear(),\n              email: event.target.email.value,\n              pseudo: event.target.pseudo.value,\n              password: event.target.mdp1.value,\n              etat: \"false\",\n              statut: \"\"\n            };\n            Meteor.call('insertInscription', hash, function (error, result) {\n              if (result) {\n                Router.go('/connexion');\n              }\n\n              if (error) {\n                console.log(error);\n              }\n            });\n          } else {\n            alert(\"Date invalide !\");\n          }\n        } else {\n          alert(\"Le pseudo que vous avez choisi est déjà utilisé !\");\n        }\n      } else {\n        alert(\"Votre mot de passe est trop court !\");\n      }\n\n      ;\n    } else {\n      alert(\"Vos mots de passe ne sont pas identiques ! \");\n    }\n  }\n});","map":{"version":3,"sources":["client/inscription/inscription.js"],"names":["Template","inscription","rendered","document","title","Tracker","autorun","Inscription","find","user","findOne","_id","Session","get","etat","sessionID","heure","Date","heureDeco","getTime","Meteor","call","set","Router","go","events","event","preventDefault","stopPropagation","mdp1","target","value","mdp2","pseudo","pseudoDb","age","$","val","nouveau","auj","length","getFullYear","alert","hash","nom","name","prenom","date","email","password","statut","error","result","console","log"],"mappings":"AAAAA,SAASC,WAAT,CAAqBC,QAArB,GAAgC,YAAW;AACzCC,WAASC,KAAT,GAAiB,YAAjB;AAEAC,UAAQC,OAAR,CAAgB,YAAW;AACzBC,gBAAYC,IAAZ;AAEA,QAAIC,OAAOF,YAAYG,OAAZ,CAAoB;AAC7BC,WAAKC,QAAQC,GAAR,CAAY,QAAZ,CADwB;AAE7BC,YAAM;AAFuB,KAApB,CAAX;;AAKA,QAAIL,IAAJ,EAAU;AACR,UAAIM,YAAYH,QAAQC,GAAR,CAAY,QAAZ,CAAhB;AACA,UAAIG,QAAQ,IAAIC,IAAJ,EAAZ;AACA,UAAIC,YAAYF,MA