stopPropagation();\n    var trouver = Inscription.findOne({\n      _id: Session.get(\"contactID\")\n    });\n\n    if (trouver) {\n      var newSurnom = event.target.surnom.value;\n      var userIdNow = Session.get(\"userID\");\n      var contact = Session.get(\"contactID\");\n\n      if (newSurnom) {\n        Meteor.call('modifierSurnom', userIdNow, contact, newSurnom, function () {});\n        Router.go('/contact');\n        LocalStore.set(\"contactID\", null);\n      }\n    }\n  },\n  'click #supprimer': function () {\n    var sessionID = LocalStore.get(\"userID\");\n    var contactID = LocalStore.get(\"contactID\");\n\n    if (confirm(\"Etes-vous sûr de vouloir supprimer ce contact\")) {\n      Meteor.call('supprimerContact', sessionID, contactID, function () {\n        Meteor.call('supprimerMessage1', sessionID, contactID, function () {\n          Meteor.call('supprimerMessage2', sessionID, contactID, function () {\n            alert(\"Contact supprimé !\");\n            Router.go('/contact');\n          });\n        });\n      });\n    } else {\n      Router.go('/modifier');\n    }\n  }\n});","map":{"version":3,"sources":["client/modifier/modifier.js"],"names":["Template","modifier","rendered","document","title","sessionID","Session","get","find","Connexion","findOne","userIdNow","Router","go","helpers","nom","newContact","Inscription","_id","prenom","age","pseudo","email","events","event","preventDefault","stopPropagation","trouver","newSurnom","target","surnom","value","contact","Meteor","call","LocalStore","set","contactID","confirm","alert"],"mappings":"AAAAA,SAASC,QAAT,CAAkBC,QAAlB,GAA6B,YAAW;AACtCC,WAASC,KAAT,GAAiB,UAAjB;AACA,MAAIC,YAAYC,QAAQC,GAAR,CAAY,QAAZ,CAAhB;AACA,MAAIC,OAAOC,UAAUC,OAAV,CAAkB;AAC3BC,eAAWN;AADgB,GAAlB,CAAX;;AAGA,MAAI,CAACA,SAAD,IAAcG,QAAQA,KAAKG,SAA/B,EAA0C;AACxCC,WAAOC,EAAP,CAAU,YAAV;AACD;AACF,CATD;;AAYAb,SAASC,QAAT,CAAkBa,OAAlB,CAA0B;AACxBC,OAAM,YAAU;AACd,QAAIC,aAAaC,YAAYP,OAAZ,CAAoB;AACnCQ,WAAMZ,QAAQC,GAAR,CAAY,WAAZ;AAD6B,KAApB,CA