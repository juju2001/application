;\n    event.stopPropagation();\n    var identifiant = Inscription.findOne({\n      _id: this._id\n    });\n    var id = identifiant._id;\n\n    if (id) {\n      Session.set(\"newContactID\", id);\n      Router.go('/newContact');\n    }\n  }\n});","map":{"version":3,"sources":["client/accueil/accueil.js"],"names":["Template","accueil","rendered","document","title","sessionID","Session","get","id","LocalStore","heure","Date","heureDeco","getTime","Meteor","call","helpers","prenomAccueil","find","Inscription","findOne","_id","prenom","contacter","contacts","Contact","userIdNow","fetch","ids","_","pluck","push","$nin","connecté","etat","contact","$in","events","event","preventDefault","stopPropagation","identifiant","set","Router","go"],"mappings":"AAAAA,SAASC,OAAT,CAAiBC,QAAjB,GAA4B,YAAW;AACrCC,WAASC,KAAT,GAAiB,SAAjB;AACA,MAAIC,YAAYC,QAAQC,GAAR,CAAY,QAAZ,CAAhB;AACA,MAAIC,KAAKC,WAAWF,GAAX,CAAe,IAAf,CAAT;;AACA,MAAGF,aAAaG,EAAhB,EAAoB;AAClB,QAAIE,QAAQ,IAAIC,IAAJ,EAAZ;AACA,QAAIC,YAAYF,MAAMG,OAAN,EAAhB;AACAC,WAAOC,IAAP,CAAY,MAAZ,EAAoBV,SAApB;AACAS,WAAOC,IAAP,CAAY,WAAZ,EAAyBV,SAAzB,EAAoCO,SAApC;AACD;AACF,CAVD;;AAaAZ,SAASC,OAAT,CAAiBe,OAAjB,CAAyB;AACvBC,iBAAe,YAAW;AACxB,QAAIZ,YAAYC,QAAQC,GAAR,CAAY,QAAZ,CAAhB;AACA,QAAIW,OAAOC,YAAYC,OAAZ,CAAoB;AAC7BC,WAAKhB;AADwB,KAApB,CAAX;;AAIA,QAAIa,IAAJ,EAAU;AACR,aAAOA,KAAKI,MAAZ;AACD;AACF,GAVsB;AAYvBC,aAAW,YAAW;AACpB,QAAIlB,YAAYC,QAAQC,GAAR,CAAY,QAAZ,CAAhB;AAEA,QAAIiB,WAAWC,QAAQP,IAAR,CAAa;AAC1BQ,iBAAWrB;AADe,KAAb,EAEZsB,KAFY,EAAf;;AAGA,QAAIC,MAAMC,EAAEC,KAAF,CAAQN,QAAR,EAAkB,SAAlB,CAAV;;AACAI,QAAIG,IAAJ,CAAS1B,SAAT;AACA,WAAOc,YAAYD,IAAZ,CAAiB;AACtBG,WAAK;AACHW,cAAMJ;AADH;AADiB,KAAjB,EAIJD,KAJI,EAAP;AAKD,GAzBsB;AA2BvBM,YAAU,YAAW;AACnB,QAAI5B,YAAYC,QAAQC,GAAR,CAAY,QAAZ,CAAhB;AACA,QAAIiB,WAAWL,YAAYD,IAAZ,CAAiB;AAC9BgB,YAAO;AADuB,KAAjB,EAEZP,KAFY,EAAf;;AAGA,QAAIC,MAAMC,EAAEC,KAAF,CAAQN,QAAR,EAAkB,KAAlB,CAAV;;AACAI,QAAIG,IAAJ,CAAS1B,SAAT;AACA,WAAOoB,QAAQP,IAAR,CAAa;AAClBiB,eAAS;AACPC,aAAKR;AADE,OADS;AAIlBF,iBAAWrB;AAJO,KAAb,EAKJsB,KALI,EAAP;AAMD;AAxCsB,CAAzB;AA2CA3B,SAASC,OAAT,CAAiBoC,MAAjB,CAAwB;AACtB,sBAAoB,UAASC,KAAT,EAAgB;AAClCA,UAAMC,cAAN;AACAD,UAAME,eAAN;AACA,QAAIC,cAActB,YAAYC,OAAZ,CAAoB;AACpCC,WAAK,KAAKA;AAD0B,KAApB,CAAlB;AAGA,QAAIb,KAAKiC,YAAYpB,GAArB;;AACA,QAAIb,EAAJ,EAAQ;AACNF,cAAQoC,GAAR,CAAY,cAAZ,EAA4BlC,EAA5B;AACAmC,aAAOC,EAAP,CAAU,aAAV;AACD;AACF;AAZqB,CAAxB","file":"client/accueil/accueil.js.map","sourcesContent":["Template.accueil.rendered = function() {\n  document.title = \"Accueil\";\n  var sessionID = Session.get(\"userID\");\n  var id = LocalStore.get(\"ID\");\n  if(sessionID != id ){\n    var heure = new Date();\n    var heureDeco = heure.getTime();\n    Meteor.call('deco', sessionID);\n    Meteor.call('heureDeco', sessionID, heureDeco);\n  }\n};\n\n\nTemplate.accueil.helpers({\n  prenomAccueil: function() {\n    var sessionID = Session.get(\"userID\");\n    var find = Inscription.findOne({\n      _id: sessionID,\n    });\n\n    if (find) {\n      return find.prenom;\n    }\n  },\n\n  contacter: function() {\n    var sessionID = Session.get(\"userID\");\n\n    var contacts = Contact.find({\n      userIdNow: sessionID,\n    }).fetch();\n    var ids = _.pluck(contacts, 'contact');\n    ids.push(sessionID);\n    return Inscription.find({\n      _id: {\n        $nin: ids,\n      },\n    }).fetch();\n  },\n\n  connecté: function() {\n    var sessionID = Session.get(\"userID\");\n    var contacts = Inscription.find({\n      etat:  \"true\",\n    }).fetch();\n    var ids = _.pluck(contacts, '_id');\n    ids.push(sessionID);\n    return Contact.find({\n      contact: {\n        $in: ids,\n      },\n      userIdNow: sessionID,\n    }).fetch();\n  },\n});\n\nTemplate.accueil.events({\n  'click .goAjouter': function(event) {\n    event.preventDefault();\n    event.stopPropagation();\n    var identifiant = Inscription.findOne({\n      _id: this._id,\n    });\n    var id = identifiant._id;\n    if (id) {\n      Session.set(\"newContactID\", id);\n      Router.go('/newContact');\n    }\n  },\n})